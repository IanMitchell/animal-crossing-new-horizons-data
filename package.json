{
  "name": "animal-crossing-new-horizons-data",
  "homepage": "https://newhorizons.gautrowski.com",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@material-ui/core": "^4.9.8",
    "@material-ui/icons": "^4.9.1",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.5.0",
    "@testing-library/user-event": "^7.2.1",
    "bootstrap": "^4.4.1",
    "fuse.js": "^5.0.10-beta",
    "jquery": "^3.4.1",
    "lodash": "^4.17.15",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-scripts": "3.4.1",
    "typescript": "^3.8.3"
  },
  "scripts": {
    "prestart": "npm run preinstall",
    "start": "react-scripts start",
    "preinstall": "cat data/bugs.jsonl | (echo 'export default ' && scripts/to-json-array) > src/bugs.js; cat data/fish.jsonl | (echo 'export default ' && scripts/to-json-array) > src/fish.js",
    "prebuild": "npm run preinstall",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "gh-pages": "^2.2.0"
  }
}
